<Window x:Class="DiplomsView.Catalog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DiplomsView"
        mc:Ignorable="d"
        Title="Catalog" Height="600" Width="400" ResizeMode="NoResize">
    <Grid>
        <Label x:Name="catalog_name" Content="Label" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="372" Height="40"/>
        <ListBox x:Name="catalog" HorizontalAlignment="Left" Height="428" Margin="10,55,0,0" VerticalAlignment="Top" Width="372"/>
        <Button x:Name="btn_addElem" Content="Добавить" HorizontalAlignment="Left" Margin="135,511,0,0" VerticalAlignment="Top" Width="120" Height="40" Click="btn_addElem_Click"/>

    </Grid>
</Window>
