<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:DiplomsView.Resources" >
    <Style x:Key="filter_check" TargetType="{x:Type CheckBox}">
        <Setter Property="FontSize" Value="14"/>
    </Style>
    <Style x:Key="add_combos" TargetType="{x:Type ComboBox}">
        <Setter Property="Width" Value="250"/>
        <Setter Property="Height" Value="40"/>
        <Setter Property="HorizontalAlignment" Value="Right"/>
    </Style>
    <Style x:Key="admin_btn" TargetType="Button">
        <Setter Property="Background" Value="White"/>
        <Setter Property="BorderBrush" Value="Black"/>
        <Setter Property="BorderThickness" Value="2"/>
        <Setter Property="FontSize" Value="14"/>
        
    </Style>
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="filterCombos.xaml"/>
        <ResourceDictionary Source="addFormCombos.xaml"/>
    </ResourceDictionary.MergedDictionaries>
</ResourceDictionary>